{
  "workbench.editor.enablePreview": true,
  "editor.formatOnSave": true,
  "editor.rulers": [
    {
      "column": 72
    },
    {
      "column": 99,
      "color": "#E74C3C"
    }
  ],
  "extensions.ignoreRecommendations": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.sql": "sql",
    "*.envrc": "shellscript",
    "*.avsc": "json"
  },

  "files.exclude": {
    "**/.asdf": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.venv": true
  },
  "files.autoSave": "onFocusChange",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "prettier.disableLanguages": ["markdown"],
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": ["----contextmanager-decorators=contextlib.contextmanager"],
  "python.linting.pylintPath": "~/.local/bin/pylint",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "~/.local/bin/flake8",
  "python.formatting.autopep8Path": "~/.local/bin/autopep8",
  "python.formatting.blackPath": "~/.local/bin/black",
  "python.formatting.blackArgs": ["--line-length", "99"],
  "python.testing.unittestEnabled": false,
  "python.testing.nosetestsEnabled": false,
  "python.testing.pytestEnabled": false,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.formatting.provider": "black",
  "python.linting.ignorePatterns": [
    ".vscode/*.py",
    "**/site-packages/**/*.py",
    "**/.asdf",
    "**/.venv"
  ],
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "turboConsoleLog.logMessagePrefix": "TCL",
  "go.coverOnSave": false,
  "go.formatTool": "goimports",
  "workbench.startupEditor": "newUntitledFile",
  "diffEditor.ignoreTrimWhitespace": false,
  "cSpell.userWords": [
    "PYPI",
    "PYTHONPATH",
    "Pipfile",
    "Popen",
    "Replicaset",
    "Sergej",
    "VENV",
    "amazonaws",
    "asyncio",
    "asyncpg",
    "boto",
    "botocore",
    "camptocamp",
    "chartmuseum",
    "chdir",
    "chmod",
    "circleci",
    "coro",
    "datasource",
    "datasources",
    "deduplicated",
    "deduplication",
    "delenv",
    "direnv",
    "dotenv",
    "elif",
    "entrypoint",
    "envrc",
    "from",
    "frser",
    "getcurrent",
    "grafana",
    "groupby",
    "gunicorn",
    "hardcoded",
    "htpasswd",
    "httpretty",
    "itertools",
    "jsonify",
    "junit",
    "kube",
    "kubeconfig",
    "kubectl",
    "kubernetes",
    "logbox",
    "mixins",
    "moto",
    "mqtt",
    "noqa",
    "opcua",
    "openapi",
    "paho",
    "pgdg",
    "pipenv",
    "postgres",
    "postgresql",
    "psql",
    "psycopg",
    "pyarrow",
    "pydict",
    "pyenv",
    "pytest",
    "rebalance",
    "rebalances",
    "repos",
    "rtype",
    "semtag",
    "setuptools",
    "sqlite",
    "strftime",
    "strptime",
    "struct",
    "terragrunt",
    "timeframe",
    "timestamptz",
    "tqdm",
    "tzinfo",
    "tzutc",
    "unmocked",
    "virtualenvs",
    "werkzeug",
    "wsgi",
    "wurstmeister",
    "xunit",
    "zlab"
  ],
  "liveshare.openSharedServers": false,
  "go.useLanguageServer": true,
  "terminal.integrated.shell.linux": "/usr/bin/bash",
  "markdownlint.config": {
    "MD024": false
  },
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.asdf": true,
    "**/.bloop": true,
    "**/.metals": true,
    "**/.venv": true
  },
  "java.project.importHint": false
}
