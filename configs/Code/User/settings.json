{
  "editor.formatOnSave": true,
  "terminal.integrated.fontWeight": "600",
  "editor.rulers": [
    100
  ],
  "extensions.ignoreRecommendations": false,
  "explorer.confirmDragAndDrop": false,
  "window.zoomLevel": 0,
  "files.associations": {
    "*.sql": "sql"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "prettier.trailingComma": "es5",
  "prettier.disableLanguages": [],
  "prettier.printWidth": 100,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "----contextmanager-decorators=contextlib.contextmanager"
  ],
  "python.linting.pylintPath": "/home/sergej/.local/bin/pylint",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=100"
  ],
  "python.linting.flake8Path": "/home/sergej/.local/bin/flake8",
  "python.formatting.autopep8Args": [
    "--max-line-length=100"
  ],
  "python.formatting.autopep8Path": "/home/sergej/.local/bin/autopep8",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "vs-kubernetes": {
    "disable-linters": [
      "resource-limits"
    ],
    "vs-kubernetes.minikube-path": "/home/sergej/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.coverOnSave": false,
  "go.formatTool": "goimports"
}